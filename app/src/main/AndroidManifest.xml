<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raymond.memorymonster">

    <application
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.raymond.memorymonster.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.raymond.memorymonster.ServiceA" android:process=":sA" />
        <service android:name="com.raymond.memorymonster.ServiceB" android:process=":sB" />
        <service android:name="com.raymond.memorymonster.ServiceC" android:process=":sC" />
        <service android:name="com.raymond.memorymonster.ServiceD" android:process=":sD" />
        <service android:name="com.raymond.memorymonster.ServiceE" android:process=":sE" />
        <service android:name="com.raymond.memorymonster.ServiceF" android:process=":sF" />
    </application>

</manifest>